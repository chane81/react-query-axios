{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "node": "current" }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "~/utils": "./src/utils",
          "~/src": "./src"
        }
      }
    ]
  ],
  "ignore": ["./node_modules"]
}
